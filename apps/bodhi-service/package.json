{
  "name": "bodhi-service",
  "description": "The Bodhi Service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "start": "NODE_ENV=staging nest start",
    "start:dev": "rimraf dist && NODE_ENV=development nest start --watch",
    "start:debug": "rimraf dist && NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node src/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "precommit": "lint-staged && npm run lint && npm run test"
  },
  "dependencies": {
    "@isnolan/bodhi-adapter": "workspace:^",
    "@isnolan/bodhi-puppet": "workspace:^",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/platform-ws": "^9.3.8",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.8",
    "@socket.io/redis-adapter": "^8.1.0",
    "ali-oss": "^6.18.0",
    "bull": "^4.10.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^16.4.2",
    "hashids": "^2.3.0",
    "https-proxy-agent": "^7.0.2",
    "ioredis": "^5.3.2",
    "mammoth": "^1.7.1",
    "mime-types": "^2.1.35",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.1.2",
    "nest-winston": "^1.7.0",
    "node-fetch": "^2.7.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf2pic": "^3.1.1",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.0",
    "tesseract.js": "^5.0.5",
    "tiktoken": "^1.0.10",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/ali-oss": "^6.16.11",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "18.0.3",
    "@types/node-fetch": "^2.6.10",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.7",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "28.1.2",
    "rimraf": "3.0.2"
  }
}
